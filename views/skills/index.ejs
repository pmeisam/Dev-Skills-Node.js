<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Skills</title>
</head>
<body>
    <!-- <h1>Hello</h1> -->
    <h1>Skills</h1>
    <ul>
        <%skills.forEach(function(s,id){%>
            <li>
                <a href="/skills/<%= id %>"> <%=s.skill%></a>
                <!-- <%=s.skill%> -->
                -
                <%=s.done ? 'done' : 'not done' %>
            </li>
        <%});%>
    </ul>
</body>
</html>